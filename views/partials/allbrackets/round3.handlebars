<div class="col s12 m3 round">
        <div class="row rd-row">
            <h5>Round 3</h5>
            <div class="end-btn">
                <button data-id="{{dbBracket.id}}" data-rd="3" class="btn waves-effect waves-light close-btn" type="submit" name="action">Close Bracket</button>
            </div>
        </div>
        <div class="row pairing">
            <div class="col s12">
                <div class="row">
                    <div class="col s6 m12 first">
                        <p>{{dbBracket.MatchUps.[6].option1}} | Current Votes: {{dbBracket.MatchUps.[6].option1_votes}}</p>
                        <button class="vote" data-for="{{dbBracket.MatchUps.[6].option1}}" data-not="{{dbBracket.MatchUps.[6].option2}}" data-id="{{dbBracket.id}}">vote</button>
                    </div>
                    <div class="col s6 m12 second">
                        <p>{{dbBracket.MatchUps.[6].option2}} | Current Votes: {{dbBracket.MatchUps.[6].option2_votes}}</p>
                        <button class="vote" data-for="{{dbBracket.MatchUps.[6].option2}}" data-not="{{dbBracket.MatchUps.[6].option1}}" data-id="{{dbBracket.id}}">vote</button>
                    </div>
                </div>
            </div>
        </div>
    </div>